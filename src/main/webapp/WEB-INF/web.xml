<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>HelloWorldFromMaven</servlet-name>
        <servlet-class>spatora.MyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HelloWorldFromMavenTwo</servlet-name>
        <servlet-class>spatora.MyServletTwo</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HelloWorldFromMaven</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloWorldFromMaven</servlet-name>
        <url-pattern>/hi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloWorldFromMaven</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloWorldFromMavenTwo</servlet-name>
        <url-pattern>/another</url-pattern>
    </servlet-mapping>
</web-app>
